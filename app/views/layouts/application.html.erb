<!DOCTYPE html>
<html>
<head>
  <title>Parrot Website</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class="navbar navbar-fixed-top navbar-default">
    <div class="container">
      <%= link_to "Parrot", "", class: "btn btn-lg btn-success", id: "logo" %>
      <div class="nav_item">
        <%= link_to "Drones", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "Minidrones", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "Zik Headphones ", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "Connected Gardening", "", class: "btn btn-default btn-warning" %>
      </div>
      <div class="nav_item">
        <%= link_to "Car Kits", "", class: "btn btn-default btn-warning" %>
      </div>
    </div>
  </header>
  <div class="container">
       <%= yield %>
       <div class="footer">
         <p>&copy; Parrot India 2016</p>
       </div>
       
</div>
</body>
</html>